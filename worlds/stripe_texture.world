<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">

    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    
    <!-- A stripe texture -->
    <include>
      <static>true</static>
        <pose>0 0 1 0 0 0</pose>
      <uri>model://stripe_texture</uri>
    </include>
    
  </world>
</sdf>